        {
            "id": 3,
            "title": "CPU Usage (%)",
            "type": "timeseries",
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 10
            },
            "options": {
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom",
                    "calcs": [
                        "mean",
                        "max",
                        "last"
                    ]
                },
                "tooltip": {
                    "mode": "multi",
                    "sort": "desc"
                }
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "unit": "percent",
                    "min": 0,
                    "max": 100,
                    "custom": {
                        "drawStyle": "line",
                        "fillOpacity": 10,
                        "lineWidth": 2
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 70
                            },
                            {
                                "color": "red",
                                "value": 90
                            }
                        ]
                    }
                }
            },
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${DS_PROMETHEUS}"
                    },
                    "expr": "rate(container_cpu_usage_seconds_total{name=~\"$pod\"}[5m]) * 100",
                    "legendFormat": "{{name}}",
                    "refId": "A"
                }
            ]
        },

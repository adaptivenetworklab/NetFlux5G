    "graphTooltip": 0,
    "id": null,
    "links": [],
    "liveNow": true,
    "panels": [
        {
            "id": 1,
            "title": "System Overview",
            "type": "stat",
            "datasource": {
                "type": "prometheus",
                "uid": "${DS_PROMETHEUS}"
            },
            "gridPos": {
                "h": 4,
                "w": 24,
                "x": 0,
                "y": 0
            },
            "options": {
                "reduceOptions": {
                    "values": false,
                    "calcs": ["lastNotNull"],
                    "fields": ""
                },
                "orientation": "horizontal",
                "textMode": "auto",
                "colorMode": "background",
                "graphMode": "none",
                "justifyMode": "auto"
            },
            "fieldConfig": {
                "defaults": {
                    "mappings": [],
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "red",
                                "value": 0
                            },
                            {
                                "color": "green",
                                "value": 1
                            }
                        ]
                    }
                },
                "overrides": [
                    {
                        "matcher": {
                            "id": "byName",
                            "options": "Total Containers"
                        },
                        "properties": [
                            {
                                "id": "unit",
                                "value": "short"
                            }
                        ]
                    }
                ]
            },
            "targets": [
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${DS_PROMETHEUS}"
                    },
                    "expr": "count(container_last_seen{name!=\"\"})",
                    "legendFormat": "Total Containers",
                    "refId": "A"
                },
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${DS_PROMETHEUS}"
                    },
                    "expr": "up{job=\"prometheus\"}",
                    "legendFormat": "Prometheus Status",
                    "refId": "B"
                },
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${DS_PROMETHEUS}"
                    },
                    "expr": "up{job=\"cadvisor\"}",
                    "legendFormat": "cAdvisor Status",
                    "refId": "C"
                },
                {
                    "datasource": {
                        "type": "prometheus",
                        "uid": "${DS_PROMETHEUS}"
                    },
                    "expr": "up{job=\"node_exporter\"}",
                    "legendFormat": "Node Exporter Status",
                    "refId": "D"
